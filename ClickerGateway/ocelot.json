{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5163
        }
      ],
      "UpstreamPathTemplate": "/Auth/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "SwaggerKey": "ClickerAuth"
    },
    {
      "DownstreamPathTemplate": "/api/Clicker/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5265
        }
      ],
      "UpstreamPathTemplate": "/Clicker/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "SwaggerKey": "Clicker",
      "AuthenticationOptions": {
        "AuthenticationProviderKeys": [ "Bearer" ]
      }
    }
  
  ],
  "SwaggerEndPoints": [
    {
      "Key": "ClickerAuth",
      "Config": [
        {
          "Url": "http://localhost:5163/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "Clicker",
      "Config": [
        {
          "Url": "http://localhost:5265/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000/"
  }
}
